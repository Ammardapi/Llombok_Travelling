<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MountainActivity">


    <ImageView
        android:id="@+id/iv_rinjani"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/rinjani" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_toEndOf="@+id/iv_rinjani"
        android:text="Gunung Rinjani"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/iv_rinjani"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />


    <TextView
        android:id="@+id/tv_jrkrinjani"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Maps"
        app:layout_constraintStart_toEndOf="@+id/iv_rinjani"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <ImageView
        android:id="@+id/iv_pergasingan"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_rinjani"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/pergasingan" />

    <TextView
        android:id="@+id/tv_pergasingan"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_toEndOf="@+id/iv_rinjani"
        android:text=" Bukit Pergasingan"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/iv_pergasingan"
        app:layout_constraintTop_toBottomOf="@+id/iv_rinjani" />


    <TextView
        android:id="@+id/tv_jarakpergasingan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Maps"
        app:layout_constraintStart_toEndOf="@+id/iv_pergasingan"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <ImageView
        android:id="@+id/iv_sempana"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_pergasingan"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/sempana" />

    <TextView
        android:id="@+id/tv_tunak"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_toEndOf="@+id/iv_rinjani"
        android:text="TWA Gunung Tunak"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/iv_tunak"
        app:layout_constraintTop_toBottomOf="@+id/iv_nanggi"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/tv_jrktunak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Maps"
        app:layout_constraintStart_toEndOf="@+id/iv_tunak"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/iv_nanggi"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_sempana"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/nanggi" />


    <TextView
        android:id="@+id/tv_sempana"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_toEndOf="@+id/iv_rinjani"
        android:text="Bukit Sempana"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/iv_sempana"
        app:layout_constraintTop_toBottomOf="@+id/iv_pergasingan" />

    <TextView
        android:id="@+id/tv_jrknanggi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Maps"
        app:layout_constraintStart_toEndOf="@+id/iv_nanggi"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/iv_tunak"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_nanggi"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/tunak" />

    <TextView
        android:id="@+id/tv_nanggi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_toEndOf="@+id/iv_sempana"
        android:text="Bukit Nanggi"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/iv_nanggi"
        app:layout_constraintTop_toBottomOf="@+id/iv_sempana" />

    <TextView
        android:id="@+id/tv_jrksempana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Maps"
        app:layout_constraintStart_toEndOf="@+id/iv_sempana"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="serif-monospace"
        android:text="Destinasi WIsata Gunung"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="24dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Sembalun, Lombok Timur"
        app:layout_constraintStart_toEndOf="@+id/iv_sempana"
        app:layout_constraintTop_toBottomOf="@+id/tv_sempana" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Sembalun, Lombok Timur"
        app:layout_constraintStart_toEndOf="@+id/iv_nanggi"
        app:layout_constraintTop_toBottomOf="@+id/tv_nanggi" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Lombok Tengah"
        app:layout_constraintStart_toEndOf="@+id/iv_tunak"
        app:layout_constraintTop_toBottomOf="@+id/tv_tunak" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Sembalun, Lombok Timur"
        app:layout_constraintStart_toEndOf="@+id/iv_pergasingan"
        app:layout_constraintTop_toBottomOf="@+id/tv_pergasingan" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Sembalun, Lombok Timur"
        app:layout_constraintStart_toEndOf="@+id/iv_rinjani"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />


</androidx.constraintlayout.widget.ConstraintLayout>