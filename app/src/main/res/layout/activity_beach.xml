<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="24dp"
    tools:context=".BeachActivity">


    <ImageView
        android:id="@+id/iv_senggigi"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/senggigi" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="32dp"
        android:layout_toEndOf="@+id/iv_senggigi"
        android:text="Senggigi Beach"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/iv_senggigi"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />


    <TextView
        android:id="@+id/tv_jrksenggigi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="6dp"
        android:text="Maps"
        app:layout_constraintStart_toEndOf="@+id/iv_senggigi"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <ImageView
        android:id="@+id/iv_kuta"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_senggigi"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/kuta" />

    <TextView
        android:id="@+id/tv_kuta"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="24dp"
        android:layout_toEndOf="@+id/iv_senggigi"
        android:text="Kuta Beach"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/iv_kuta"
        app:layout_constraintTop_toBottomOf="@+id/iv_senggigi" />


    <TextView
        android:id="@+id/tv_jarakkuta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="6dp"
        android:text="Maps"
        app:layout_constraintStart_toEndOf="@+id/iv_kuta"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/iv_sempana"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_kuta"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/trawangan" />

    <TextView
        android:id="@+id/tv_tunak"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="24dp"
        android:layout_toEndOf="@+id/iv_senggigi"
        android:text="Gili Meno"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/iv_nanggi"
        app:layout_constraintTop_toBottomOf="@+id/iv_mawun"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/tv_jrkmeno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="8dp"
        android:text="Maps"
        app:layout_constraintStart_toEndOf="@+id/iv_nanggi"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/iv_nanggi"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_mawun"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/meno" />

    <TextView
        android:id="@+id/tv_sempana"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="24dp"
        android:layout_toEndOf="@+id/iv_senggigi"
        android:text="Mawun Beach"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/iv_mawun"
        app:layout_constraintTop_toBottomOf="@+id/iv_sempana" />


    <TextView
        android:id="@+id/tv_jrkmawun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="8dp"
        android:text="Maps"
        app:layout_constraintStart_toEndOf="@+id/iv_mawun"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/iv_mawun"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_sempana"
        tools:ignore="MissingConstraints"
        tools:srcCompat="@drawable/mawun" />

    <TextView
        android:id="@+id/tv_nanggi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="24dp"
        android:layout_toEndOf="@+id/iv_sempana"
        android:text="Gili Trawangan"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/iv_sempana"
        app:layout_constraintTop_toBottomOf="@+id/iv_kuta" />

    <TextView
        android:id="@+id/tv_jrktrawangan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="8dp"
        android:text="Maps"
        app:layout_constraintStart_toEndOf="@+id/iv_sempana"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:fontFamily="serif-monospace"
        android:text="Beach Tourist Destination"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="24dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="6dp"
        android:text="Lombok Tengah"
        app:layout_constraintStart_toEndOf="@+id/iv_kuta"
        app:layout_constraintTop_toBottomOf="@+id/tv_kuta" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="6dp"
        android:text="Lombok Tengah"
        app:layout_constraintStart_toEndOf="@+id/iv_mawun"
        app:layout_constraintTop_toBottomOf="@+id/tv_sempana" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="6dp"
        android:text="Lombok Utara"
        app:layout_constraintStart_toEndOf="@+id/iv_nanggi"
        app:layout_constraintTop_toBottomOf="@+id/tv_tunak" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="6dp"
        android:text="Lombok Utara"
        app:layout_constraintStart_toEndOf="@+id/iv_sempana"
        app:layout_constraintTop_toBottomOf="@+id/tv_nanggi" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="6dp"
        android:text="Lombok Barat"
        app:layout_constraintStart_toEndOf="@+id/iv_senggigi"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

</androidx.constraintlayout.widget.ConstraintLayout>







